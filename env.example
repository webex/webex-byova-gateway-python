# BYOVA Gateway Environment Variables
# Copy this file to .env and fill in your actual values

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_SESSION_TOKEN=your_session_token_here  # Optional, for temporary credentials

# AWS Lex Configuration
AWS_LEX_BOT_ALIAS_ID=TSTALIASID
AWS_LEX_BOT_NAME=your_bot_name
AWS_LEX_LOCALE=en_US

# Gateway Configuration
GATEWAY_HOST=0.0.0.0
GATEWAY_PORT=50051
MONITORING_HOST=0.0.0.0
MONITORING_PORT=8080

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=/app/logs/gateway.log

# Security (for production)
# Use these for additional security in production
# JWT_SECRET=your_jwt_secret_here
# ENCRYPTION_KEY=your_encryption_key_here





